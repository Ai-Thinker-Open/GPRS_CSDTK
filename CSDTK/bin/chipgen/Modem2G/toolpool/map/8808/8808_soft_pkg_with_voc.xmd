	</entry>
	
</enum>

<enum name="GTES_API_CC_ERROR">
	
	<entry name="GTES_API_CC_ERROR_NONE" value="0x0000"/>
	<entry name="GTES_API_CC_ERROR_NORMAL_RELEASE" />
	<entry name="GTES_API_CC_ERROR_LOWER_LAYER_FAILURE"/>
