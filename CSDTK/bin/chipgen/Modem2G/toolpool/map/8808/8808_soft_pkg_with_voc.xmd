<typedef name="GTES_STACK_STATUS">
	
	<comment>
	This structure contains the main elements describing the stack status.
	</comment>
    
    
	<instance name="lastApiToMmiMsg" type="GTES_API_MSG" />
	
	<instance name="lastCcError" type="GTES_API_CC_ERROR" />
	
	<UINT8 name="lastTi" >
		<comment>
